{
  "family": "python-mid-project",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::361769594235:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "python-mid-project",
      "image": "REPLACED_BY_ACTION",
      "essential": true,
      "portMappings": [
        { "containerPort": 5000, "protocol": "tcp" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/python-mid-project",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": []
    }
  ]
}
