{
  "family": "python-mid-project",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::myproject-ci-user:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::myproject-ci-user:role/ECSTaskRole",
  "containerDefinitions": [
    {
     "name": "python-mid-project",
      "image": "361769594235.dkr.ecr.us-east-1.amazonaws.com/python-mid-project:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/python-mid-project",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": []
    }
  ]
}
